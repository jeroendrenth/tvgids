{
  "manifest_version": 2,

  "name": "TV Gids",
  "description": "Wat is er nu en straks op tv? Bekijk ook een programmaoverzicht per zender.",
  "version": "1.0.1",
  "options_page": "options.html",

  "browser_action": {
    "default_icon": "icons/tvgids.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "activeTab",
    "http://www.tvgids.nl/json/lists/channels.php",
    "storage"
  ]
}
